<!DOCTYPE html>
<html>
<head>
  <title>Moviesapp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="banner">
	<%=image_tag("rails.png", :size => "100x50") %> Movies App
</div>
<div id="nav">
	<ul>
		<li><%=link_to"Questions" %></li>
		<li><%=link_to"Movie News", 'http://www.imdb.com/' %></li>
		<li><%=link_to"Contact Us" %></li>
		<li><%=link_to"Movies", movies_path %></li>
	</ul>
</div>

<%= yield %>

<div id="footer">
	<%=image_tag("rails_f.jpg", :size =>"500x200") %>
</div>
</body>
</html>
